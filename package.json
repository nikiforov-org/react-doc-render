{
  "name": "react-doc-render",
  "description": "react-doc-render is a lightweight React library for rendering documents of various popular MIME types directly in the browser. It supports formats such as PDF, images (JPEG, PNG, GIF), text files, and Microsoft Office documents (Word, Excel, PowerPoint), providing a simple and unified interface to handle different content types.",
  "version": "0.1.38",
  "private": false,
  "author": "Evgenii Nikiforov <e@nikiforov.org>",
  "license": "MIT",
  "keywords": [
    "browser",
    "component",
    "docx",
    "document",
    "excel",
    "file",
    "image",
    "mime",
    "office",
    "pdf",
    "react",
    "render",
    "renderer",
    "rendering",
    "typescript",
    "viewer",
    "word",
    "xlsx"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "homepage": "https://github.com/nikiforov-org/react-doc-render#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikiforov-org/react-doc-render.git"
  },
  "bugs": {
    "url": "https://github.com/nikiforov-org/react-doc-render/issues"
  },
  "scripts": {
    "start": "PUBLIC_URL=/ react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-children-utilities)/\"",
    "coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "build:dist": "npm run build:types && npm run build:js",
    "build:types": "./node_modules/.bin/tsc --project ./tsconfig.types.json",
    "build:js": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --extensions \".ts,.tsx\" --source-maps false",
    "postinstall": "cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs src/lib/utils/pdf.worker.min.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "canvas-datagrid": "^0.4.7",
    "mammoth": "^1.8.0",
    "pdfjs-dist": "^4.6.82",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "react-scripts": "5.0.1",
    "sass": "^1.79.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}